{{!< default}}



<main class="blog" role="main">
    <article class="{{post_class}}">
        {{#post}}
        <header class="post-header">
            <h1 class="post-title">
            {{title}}
            </h1>
        </header>

        <section class="post-content">
            {{content}}
        </section>
        {{/post}}
    </article>

</main>